new messageB in {
	contract @{"global_factory_B"}(getB, setB) = {
		contract getB(retB) = {
			for (msgB <- messageB){
				retB!(*msgB)
				|
				messageB!(*msgB)
			}
		}
		|
		contract setB(newMessageB) = {
			for (msgB <- messageB) {
				messageB!(*newMessageB)
			}
		}
	}
	|
	messageB!("Hello")
}
